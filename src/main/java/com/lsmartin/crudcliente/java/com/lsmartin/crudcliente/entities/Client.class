package com.lsmartin.crudcliente.entities;

@Entity
@Table(name = "tb_client")
public class Client{

    @id
    @GenarationValue(strategy = "IDENTITY")
    private Long id;
    private String name;
    private String cpf;
    private Double income;
    private LocalDate birthDate;
    private Integer children;

    public Client(){

    }

    public Client(Long id, String name, String cpf, Double income, LocalDate birthDate, Integer children){
        this.id = id;
        this.name = name;
        this.cpf = cpf;
        this.income = income;
        this.birthDate = birthDate;
        this.children = children;
    }

    public void setId(Long id){
        this.id=id;
    }

    public Long getId(){
        return id;
    }

    public void setName(String name){
        this.name=name;
    }

    public Long getName(){
        return name;
    }

    public void setCpf(String cpf){
        this.cpf=cpf;
    }

    public String getCpf(){
        return cpf;
    }

    public void setIncome(Double cpf){
        this.income=income;
    }

    public Double getIncome(){
        return income;
    }

    public void setBirdDate(LocalDate birthDate){
        this.birthDate=birthDate;
    }

    public LocalDate getBirdDate(){
        return birthDate;
    }

    public void setChildren(Integer children){
        this.children=children;
    }

    public Integer getChildren(){
        return children;
    }


}